                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp" >

                    <com.stexgroup.novinki.customviews.TextViewMainTheme
                        android:id="@+id/TextViewMainTheme06"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
